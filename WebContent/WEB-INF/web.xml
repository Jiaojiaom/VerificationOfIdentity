<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>VerificationOfIdentity</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>struts</filter-name>
    <filter-class>org.apache.struts2.dispatcher.filter.StrutsPrepareAndExecuteFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>struts</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>teaAdminLogFilter</filter-name>
    <filter-class>com.bean.LoginFilter</filter-class>
    <init-param>
      <param-name>redirectURL</param-name>
      <param-value>/login.jsp</param-value>
    </init-param>
  </filter>
  <filter>
    <filter-name>stuLogFilter</filter-name>
    <filter-class>com.bean.LoginFilter</filter-class>
    <init-param>
      <param-name>redirectURL</param-name>
      <param-value>/student/login.jsp</param-value>
    </init-param>
  </filter>
  <filter>
    <filter-name>registSystemFilter</filter-name>
    <filter-class>com.bean.RegistSystemFilter</filter-class>
    <init-param>
      <param-name>redirectURL</param-name>
      <param-value>/student/login.jsp</param-value>
    </init-param>
  </filter>
  <filter>
    <filter-name>registFilter</filter-name>
    <filter-class>com.bean.RegistFilter</filter-class>
    <init-param>
      <param-name>redirectURL</param-name>
      <param-value>/student/examArrage.jsp</param-value>
    </init-param>
  </filter>
  <filter>
    <filter-name>identificationSystemFilter</filter-name>
    <filter-class>com.bean.identificationSystemFilter</filter-class>
    <init-param>
      <param-name>redirectURL</param-name>
      <param-value>/login.jsp</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>teaAdminLogFilter</filter-name>
    <url-pattern>/teacher/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>identificationSystemFilter</filter-name>
    <url-pattern>/teacher/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>teaAdminLogFilter</filter-name>
    <url-pattern>/admin/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>stuLogFilter</filter-name>
    <url-pattern>/student/enlist.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>stuLogFilter</filter-name>
    <url-pattern>/student/editInfo.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>stuLogFilter</filter-name>
    <url-pattern>/student/examArrage.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>stuLogFilter</filter-name>
    <url-pattern>/student/stuInfo.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>stuLogFilter</filter-name>
    <url-pattern>/student/uploadImg.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>registSystemFilter</filter-name>
    <url-pattern>/student/editInfo.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>registSystemFilter</filter-name>
    <url-pattern>/student/enlist.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>registSystemFilter</filter-name>
    <url-pattern>/student/examArrage.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>registSystemFilter</filter-name>
    <url-pattern>/student/stuInfo.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>registSystemFilter</filter-name>
    <url-pattern>/student/uploadImg.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>registSystemFilter</filter-name>
